# Device tree
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# File systems
CONFIG_EXFAT_FS=y

# Fingerprint
CONFIG_FINGERPRINT_GOODIX_FOD=m
CONFIG_INPUT_FINGERPRINT=y

# Haptics
# CONFIG_INPUT_QCOM_HV_HAPTICS is not set
CONFIG_INPUT_AW862XX_HAPTIC=y

# Hardware ID
CONFIG_MI_HARDWARE_ID=m

# IR
CONFIG_IR_SPI=m
CONFIG_LIRC=y
CONFIG_RC_CORE=y
CONFIG_RC_DECODERS=y
CONFIG_RC_DEVICES=y

# Pinctrl
CONFIG_PINCTRL_SM7325=y

# Power supply
# CONFIG_BACKLIGHT_QCOM_SPMI_WLED is not set
CONFIG_BQ2597X=y
CONFIG_BQ_FG_1S=y
CONFIG_BQ_FUEL_GAUGE=y
CONFIG_LEDS_QTI_FLASH=m
CONFIG_QTI_BATTERY_CHARGER=m

# Proximity
CONFIG_MIUS_PROXIMITY=y

# Thermal
CONFIG_MI_THERMAL_INTERFACE=m

# Touchscreen
# CONFIG_TOUCHSCREEN_FTS is not set
# CONFIG_TOUCHSCREEN_NT36XXX is not set
# CONFIG_TOUCHSCREEN_ST is not set
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_CORE=m
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_DEVICE=m
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_DIAGNOSTIC=m
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_REFLASH=m
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_S3908P=m
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_SPI=m
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_TESTING=m
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=m

# WiFi
CONFIG_ICNSS2=m
# CONFIG_ICNSS2_RESTART_LEVEL_NOTIF is not set
CONFIG_CNSS_QCA6750=y
CONFIG_CNSS2=m
CONFIG_QCA_CLD_WLAN=m
