# Device tree
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# File systems
CONFIG_EXFAT_FS=y

# Fingerprint
CONFIG_INPUT_FINGERPRINT=y
CONFIG_FINGERPRINT_GOODIX_FOD=m

# Haptics
# CONFIG_INPUT_QCOM_HV_HAPTICS is not set
CONFIG_INPUT_AW8697_HAPTIC=y

# Hardware ID
CONFIG_MI_HARDWARE_ID=m

# IR
CONFIG_IR_SPI=m
CONFIG_LIRC=y
CONFIG_RC_CORE=y
CONFIG_RC_DECODERS=y
CONFIG_RC_DEVICES=y

# Power supply
# CONFIG_BACKLIGHT_QCOM_SPMI_WLED is not set
CONFIG_BQ2597X=y
CONFIG_BQ_FUEL_GAUGE=y
CONFIG_LEDS_QTI_FLASH=m
CONFIG_QTI_BATTERY_CHARGER=m

# Proximity
CONFIG_MIUS_PROXIMITY=y

# Thermal
CONFIG_MI_THERMAL_INTERFACE=m

# Time of flight
CONFIG_STMVL53L5=m

# Touchscreen
# CONFIG_TOUCHSCREEN_FTS is not set
# CONFIG_TOUCHSCREEN_NT36XXX is not set
# CONFIG_TOUCHSCREEN_ST is not set
CONFIG_TOUCHSCREEN_CYPRESS_CYTTSP5=m
CONFIG_TOUCHSCREEN_CYPRESS_CYTTSP5_DEVICE_ACCESS=m
CONFIG_TOUCHSCREEN_CYPRESS_CYTTSP5_I2C=m
CONFIG_TOUCHSCREEN_CYPRESS_CYTTSP5_LOADER=m
CONFIG_TOUCHSCREEN_ST_FTS_V521_SPI=m
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=m

# Ultra wideband
CONFIG_NXP_SR100=m

# WiFi
CONFIG_ICNSS2=m
# CONFIG_ICNSS2_RESTART_LEVEL_NOTIF is not set
CONFIG_CNSS2=m
CONFIG_CNSS_QCA6490=y
CONFIG_QCA_CLD_WLAN=m

# Wireless charging
CONFIG_MI_WIRELESS=y
